# Database
DATABASE_URL="postgresql://user:password@localhost:5432/nolia_funding"

# AWS Configuration
AWS_REGION="us-east-1"
AWS_ACCESS_KEY_ID="your-access-key-id"
AWS_SECRET_ACCESS_KEY="your-secret-access-key"

# S3 for file storage
S3_BUCKET_DOCUMENTS="nolia-documents-bucket"

# SQS for async document processing
SQS_DOCUMENT_PROCESSING_QUEUE="https://sqs.us-east-1.amazonaws.com/599065966827/nolia-document-processing.fifo"
SQS_BRAIN_ASSEMBLY_QUEUE="https://sqs.us-east-1.amazonaws.com/599065966827/nolia-brain-assembly.fifo"

# AWS Bedrock for AI
BEDROCK_REGION="us-east-1"

# AWS OpenSearch for RAG vector database
OPENSEARCH_ENDPOINT="https://search-nolia-funding-rag-xxxxx.us-east-1.es.amazonaws.com"
OPENSEARCH_USERNAME="admin"
OPENSEARCH_PASSWORD="your-opensearch-password"

# OpenAI for embeddings (fallback)
OPENAI_API_KEY="sk-your-openai-api-key"

# Admin access for system initialization
ADMIN_API_KEY="your-admin-api-key-for-rag-init"

# Development
NODE_ENV="development"
NEXT_PUBLIC_APP_URL="http://localhost:3000"